extends ../layout

block content
    h2 Polls
    a(href=`/poll/create`) Create
    ul
        each poll in polls
            li
                h3= poll.title
                ul
                    li
                        a(href=`/user/polls/edit/${poll.uniqueID}`) Edit
                    li
                        a(href=`/poll/vote/${poll.uniqueID}`) Live link



